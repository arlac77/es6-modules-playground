{
  "name": "es6-modules-playground",
  "description": "playing around with ES2015 modules and rollup",
  "version": "0.0.0-semantic-release",
  "scripts": {
    "test": "ava",
    "cover": "nyc --temp-directory build/nyc ava",
    "docs": "documentation readme src/B.js --section=API",
    "semantic-release": "semantic-release",
    "pretest": "rollup -c tests/rollup.config.js",
    "posttest": "npm run prepare && markdown-doctest",
    "precover": "rollup -c tests/rollup.config.js",
    "prepare": "rollup -c",
    "lint": "documentation lint src/B.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/arlac77/es6-modules-playground.git"
  },
  "bugs": {
    "url": "https://github.com/arlac77/es6-modules-playground/issues"
  },
  "homepage": "https://github.com/arlac77/es6-modules-playground#readme",
  "license": "BSD-2-Clause",
  "engines": {
    "node": ">=8.9.3"
  },
  "devDependencies": {
    "markdown-doctest": "^0.9.1",
    "semantic-release": "^11.1.0",
    "rollup": "^0.53.0",
    "rollup-plugin-commonjs": "^8.2.6",
    "rollup-plugin-node-resolve": "^3.0.0",
    "ava": "^0.24.0",
    "nyc": "^11.4.1",
    "rollup-plugin-babel": "^3.0.3",
    "rollup-plugin-multi-entry": "^2.0.2",
    "xo": "^0.19.0",
    "documentation": "^5.3.5"
  },
  "dependencies": {
    "loglevel-mixin": "^2.0.1",
    "model-attributes": "^3.0.10",
    "statetransition-mixin": "^4.0.0"
  },
  "main": "dist/index.js",
  "module": "src/B.js",
  "keywords": [],
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "ava": {
    "files": [
      "build/test-bundle.js"
    ],
    "require": [
      "babel-register"
    ]
  },
  "nyc": {
    "include": [
      "build/test-bundle.js",
      "src/**/*.js"
    ],
    "reporter": [
      "lcov"
    ],
    "report-dir": "./build/coverage"
  },
  "template": {
    "repository": {
      "url": "https://github.com/arlac77/npm-package-template.git"
    }
  },
  "xo": {
    "space": true
  }
}
