{
  "name": "es6-modules-playground",
  "description": "playing around with ES2015 modules and rollup",
  "version": "0.0.0-semantic-release",
  "scripts": {
    "build": "node_modules/rollup/bin/rollup --output=dist/index.js -c -- src/B.js",
    "test": "npm run build && node_modules/.bin/mocha tests/*_test.js && markdown-doctest",
    "cover": "npm run build && node_modules/.bin/istanbul cover node_modules/.bin/_mocha -- -u exports tests/*_test.js",
    "docs": "jsdoc2md -l off -t doc/README.hbs -f src/*.js > README.md",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/arlac77/es6-modules-playground.git"
  },
  "bugs": {
    "url": "https://github.com/arlac77/es6-modules-playground/issues"
  },
  "homepage": "https://github.com/arlac77/es6-modules-playground#readme",
  "license": "BSD-2-Clause",
  "engines": {
    "node": ">=6.9.1"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "cz-conventional-changelog": "^1.2.0",
    "istanbul": "^0.4.5",
    "mocha": "^3.2.0",
    "jsdoc-to-markdown": "^2.0.1",
    "markdown-doctest": "^0.9.1",
    "semantic-release": "^6.3.5",
    "rollup": "^0.38.0",
    "rollup-plugin-commonjs": "^6.0.1",
    "rollup-plugin-node-resolve": "^2.0.0"
  },
  "dependencies": {
    "loglevel-mixin": "^1.6.8",
    "model-attributes": "^2.2.17",
    "statetransition-mixin": "^3.1.10"
  },
  "main": "dist/index.js",
  "module": "src/B.js",
  "keywords": [],
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}