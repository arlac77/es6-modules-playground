{
  "name": "es6-modules-playground",
  "description": "playing around with ES2015 modules and rollup",
  "version": "0.0.0-semantic-release",
  "index": "dist/bundle.js",
  "scripts": {
    "build": "./node_modules/rollup/bin/rollup --output={{main}} -c -- {{module}}",
    "test": "npm run build && mocha tests/*_test.js && markdown-doctest",
    "cover": "npm run build && ./node_modules/istanbul/lib/cli.js cover --hook-run-in-context ./node_modules/mocha/bin/_mocha -- --R spec --U exports tests/*_test.js",
    "docs": "jsdoc2md -l off -t doc/README.hbs -f {{module}} > README.md",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/arlac77/es6-modules-playground.git"
  },
  "bugs": {
    "url": "https://github.com/arlac77/es6-modules-playground/issues"
  },
  "homepage": "https://github.com/arlac77/es6-modules-playground#readme",
  "license": "BSD-2-Clause",
  "engines": {
    "node": ">=6.9.1"
  },
  "devDependencies": {
    "rollup": "^0.36.4",
    "rollup-plugin-commonjs": "^5.0.5",
    "rollup-plugin-node-resolve": "^2.0.0",
    "chai": "^3.5.0",
    "cz-conventional-changelog": "^1.2.0",
    "istanbul": "^0.4.5",
    "mocha": "^3.1.2",
    "jsdoc-to-markdown": "^2.0.1",
    "markdown-doctest": "^0.9.0",
    "semantic-release": "^6.3.2"
  },
  "dependencies": {
    "loglevel-mixin": "^1.6.4",
    "model-attributes": "^2.2.11",
    "statetransition-mixin": "^3.1.5"
  },
  "main": "{{main}}",
  "keywords": [],
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}