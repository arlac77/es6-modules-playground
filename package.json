{
  "name": "es6-modules-playground",
  "description": "playing around with ES2015 modules and rollup",
  "version": "0.0.0-semantic-release",
  "scripts": {
    "test": "ava",
    "cover": "nyc ava",
    "docs": "jsdoc2md --configure doc/jsdoc.json -l off -t doc/README.hbs -f src/*.js >README.md",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "prepublish": "rollup --output=dist/index.js -c -- src/B.js",
    "pretest": "rollup -c rollup.config.test.js",
    "posttest": "npm run prepublish && markdown-doctest",
    "precover": "rollup -c rollup.config.test.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/arlac77/es6-modules-playground.git"
  },
  "bugs": {
    "url": "https://github.com/arlac77/es6-modules-playground/issues"
  },
  "homepage": "https://github.com/arlac77/es6-modules-playground#readme",
  "license": "BSD-2-Clause",
  "engines": {
    "node": ">=8.0.0"
  },
  "devDependencies": {
    "cz-conventional-changelog": "^2.0.0",
    "jsdoc-to-markdown": "^3.0.0",
    "markdown-doctest": "^0.9.1",
    "semantic-release": "^6.3.6",
    "rollup": "^0.41.6",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-node-resolve": "^3.0.0",
    "ava": "^0.19.1",
    "babel-preset-es2015-rollup": "^3.0.0",
    "nyc": "^11.0.1",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-multi-entry": "^2.0.1",
    "jsdoc-babel": "^0.3.0",
    "xo": "^0.19.0",
    "babel-preset-stage-3": "^6.24.1"
  },
  "dependencies": {
    "loglevel-mixin": "^1.6.22",
    "model-attributes": "^3.0.10",
    "statetransition-mixin": "^3.1.18"
  },
  "main": "dist/index.js",
  "module": "src/B.js",
  "keywords": [],
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "ava": {
    "files": [
      "build/test-bundle.js"
    ],
    "require": [
      "babel-register"
    ]
  },
  "nyc": {
    "include": [
      "build/test-bundle.js",
      "src/**/*.js"
    ],
    "reporter": [
      "lcov"
    ],
    "report-dir": "./coverage"
  },
  "template": {
    "repository": {
      "url": "https://github.com/arlac77/npm-package-template.git"
    }
  },
  "xo": {
    "space": true
  }
}